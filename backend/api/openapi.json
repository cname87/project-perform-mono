{
  "openapi" : "3.0.1",
  "info" : {
    "title" : "Project Perform API V1",
    "description" : "V1.x.x cover the API for one team of members.",
    "contact" : {
      "name" : "Sean Young",
      "email" : "cname@yahoo.com"
    },
    "license" : {
      "name" : "Apache 2.0",
      "url" : "http://www.apache.org/licenses/LICENSE-2.0.html"
    },
    "version" : "1.0.1"
  },
  "servers" : [ {
    "url" : "https://localhost:1337/api-v1",
    "description" : "Development server"
  } ],
  "paths" : {
    "/isTestDatabase" : {
      "get" : {
        "summary" : "Returns whether the test database is in use.",
        "description" : "Returns whether the test database is in use.\nIf the test database is in use then it is safe to run the api tests.\n",
        "operationId" : "getIsTestDatabase",
        "responses" : {
          "200" : {
            "description" : "Test mode returned.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/IsTestDatabase"
                }
              }
            }
          },
          "default" : {
            "description" : "Unexpected error.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          }
        },
        "x-swagger-router-controller" : "members"
      }
    },
    "/members" : {
      "get" : {
        "summary" : "Returns all the members, or as determined by a query string.",
        "description" : "Returns all the members, or as determined by a search string.\n",
        "operationId" : "getMembers",
        "parameters" : [ {
          "name" : "name",
          "in" : "query",
          "description" : "An optional name search string to limit the returned list.\nAll members with the name property starting with 'name' will be returned. \n",
          "required" : false,
          "style" : "form",
          "explode" : true,
          "schema" : {
            "maxLength" : 35,
            "type" : "string"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Members returned.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "type" : "array",
                  "items" : {
                    "$ref" : "#/components/schemas/Member"
                  }
                }
              }
            }
          },
          "400" : {
            "description" : "The request is not valid.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "503" : {
            "description" : "The database service is not available now.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "default" : {
            "description" : "Unexpected error.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          }
        },
        "x-swagger-router-controller" : "members"
      },
      "put" : {
        "summary" : "Updates a member (based on the id property of the member).",
        "description" : "Updates the data on a member.\n",
        "operationId" : "updateMember",
        "requestBody" : {
          "description" : "Updates a member.\nA member object is supplied which must have an id property.\nThe member with that id is updated.\n",
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/Member"
              }
            }
          },
          "required" : true,
          "x-name" : "member"
        },
        "responses" : {
          "200" : {
            "description" : "Member updated.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Member"
                }
              }
            }
          },
          "400" : {
            "description" : "The request is not a valid request.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "404" : {
            "description" : "A member with the supplied member ID was not found.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "503" : {
            "description" : "The database service is not available now.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "default" : {
            "description" : "Unexpected error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          }
        },
        "x-swagger-router-controller" : "members"
      },
      "post" : {
        "summary" : "Adds a member.",
        "description" : "Adds a supplied member.\nA member object must be supplied in the body.\nAn error (409) will be returned if a member with the same id as the id in the supplied member object already exists. \n",
        "operationId" : "addMember",
        "requestBody" : {
          "description" : "Mmber detail (without id field).",
          "content" : {
            "application/json" : {
              "schema" : {
                "$ref" : "#/components/schemas/MemberWithoutId"
              }
            }
          },
          "required" : true,
          "x-name" : "member"
        },
        "responses" : {
          "201" : {
            "description" : "Member created",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Member"
                }
              }
            }
          },
          "400" : {
            "description" : "The request is not a valid request.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "503" : {
            "description" : "The database service is not available now.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "default" : {
            "description" : "Unexpected error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          }
        },
        "x-swagger-router-controller" : "members"
      },
      "delete" : {
        "summary" : "Deletes all members.",
        "description" : "Deletes all members.\n",
        "operationId" : "deleteMembers",
        "responses" : {
          "200" : {
            "description" : "Members deleted",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Count"
                }
              }
            }
          },
          "400" : {
            "description" : "The request is not a valid request.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "503" : {
            "description" : "The database service is not available now.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "default" : {
            "description" : "Unexpected error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          }
        },
        "x-swagger-router-controller" : "members"
      }
    },
    "/members/{id}" : {
      "get" : {
        "summary" : "Returns a specific member.",
        "description" : "Get a specific member.\n",
        "operationId" : "getMember",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "description" : "The value of the id property of the member.",
          "required" : true,
          "style" : "simple",
          "explode" : false,
          "schema" : {
            "maximum" : 999,
            "minimum" : 1,
            "type" : "integer",
            "format" : "int32"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Member information returned.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Member"
                }
              }
            }
          },
          "400" : {
            "description" : "The request is not a valid request.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "404" : {
            "description" : "A member with the supplied member ID was not found.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "503" : {
            "description" : "The database service is not available now",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "default" : {
            "description" : "Unexpected error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          }
        },
        "x-swagger-router-controller" : "members"
      },
      "delete" : {
        "summary" : "Deletes a member.",
        "description" : "Deletes a member.\n",
        "operationId" : "deleteMember",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "description" : "The value of the id property of the member.",
          "required" : true,
          "style" : "simple",
          "explode" : false,
          "schema" : {
            "maximum" : 999,
            "minimum" : 1,
            "type" : "integer",
            "format" : "int32"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Member deleted",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Count"
                }
              }
            }
          },
          "400" : {
            "description" : "The request is not a valid",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "404" : {
            "description" : "A member with the supplied member ID was not found.",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "503" : {
            "description" : "The database service is not available now",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          },
          "default" : {
            "description" : "Unexpected error",
            "content" : {
              "application/json" : {
                "schema" : {
                  "$ref" : "#/components/schemas/Error"
                }
              }
            }
          }
        },
        "x-swagger-router-controller" : "members"
      }
    }
  },
  "components" : {
    "schemas" : {
      "Member" : {
        "required" : [ "id", "name" ],
        "type" : "object",
        "properties" : {
          "id" : {
            "maximum" : 999,
            "minimum" : 1,
            "type" : "integer",
            "format" : "int32",
            "example" : 5
          },
          "name" : {
            "maxLength" : 35,
            "type" : "string",
            "format" : "string",
            "example" : "John Athlete"
          }
        },
        "description" : "Member information"
      },
      "MemberWithoutId" : {
        "required" : [ "name" ],
        "type" : "object",
        "properties" : {
          "name" : {
            "maxLength" : 35,
            "type" : "string",
            "format" : "string",
            "example" : "John Athlete"
          }
        },
        "description" : "Member information without the id field"
      },
      "Error" : {
        "required" : [ "code", "message" ],
        "type" : "object",
        "properties" : {
          "code" : {
            "type" : "integer",
            "format" : "int32",
            "example" : 5
          },
          "message" : {
            "type" : "string",
            "format" : "string",
            "example" : "Not found"
          }
        },
        "description" : "Error report"
      },
      "Count" : {
        "required" : [ "count" ],
        "type" : "object",
        "properties" : {
          "count" : {
            "type" : "integer",
            "format" : "int32",
            "example" : 5
          }
        },
        "description" : "Count of documents."
      },
      "IsTestDatabase" : {
        "required" : [ "isTestDatabase" ],
        "type" : "object",
        "properties" : {
          "isTestDatabase" : {
            "type" : "boolean",
            "example" : false
          }
        },
        "description" : "boolean 'true' => test database is in use."
      }
    }
  }
}
