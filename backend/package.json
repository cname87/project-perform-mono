    "runServer": "npm run debug && npm run isServerUp",
    "delDist": "ts-node ./utils/utils-build/delDistDir.ts ./dist",
    "startServer": "ts-node --project ./tsconfig.json ./utils/utils-build/startServer.ts",
    "copyEnv": "ts-node ./utils/utils-build/copyEnv.ts ./ ./dist",
