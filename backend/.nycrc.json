{
    "all": true,
    "branches": 0,
    "cache": true,
    "check-coverage": true,
    "include": [
      "backend/dist/src/**/*.js"
    ],
    "exclude": [
      "**/test/**/*.*"
    ],
    "exclude-after-remap": false,
    "functions": 0,
    "lines": 0,
    "per-file": true,
    "produce-source-map": true,
    "report-dir": "./backend/coverage",
    "reporter": [
        "html",
        "text"
    ],
    "require": "@istanbuljs/nyc-config-typescript",
    "statements": 0,
    "temp-dir": "./backend/coverage/.nyc_output",
    "watermarks": {
        "branches": [
            60,
            80
        ],
        "functions": [
            60,
            80
        ],
        "lines": [
            60,
            80
        ],
        "statements": [
            60,
            80
        ]
    }
}

