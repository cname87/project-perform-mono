  "scripts": {
    "delDist": "ts-node --project ./utils/tsconfig.utils.json ./utils/delDistDir.ts ./dist/",
    "ngBuild:dev": "ng run project-perform:build",
    "ngBuild:e2e": "ng run project-perform:build:e2eTest",
    "ngBuild:prod": "ng run project-perform:build:production",
    "prettier": "prettier --write \"./**/*.{ts,tsx}\"",
    "tslint:app": "tslint ./src/**/*.ts --format verbose --project ./src/tsconfig.app.json --config ./src/tslint.json",
    "tslint:spec": "tslint ./src/**/spec.ts --format verbose --project ./src/tsconfig.spec.json --config ./src/tslint.json",
    "tslint": "npm run tslint:app && npm run tslint:spec",
    "build:dev": "npm-run-all delDist ngBuild:dev tslint prettier",
    "build:e2e": "npm-run-all delDist ngBuild:e2e tslint prettier",
    "build:prod": "npm-run-all delDist ngBuild:prod tslint:app prettier",
    "delay": "sleepms 2000",
    "startBackend": " cd .. && npm run checkServer",
    "serve": "ng run project-perform:serve",
    "serveWithDelay": "npm run delay && npm run serve",
    "serveWithBackend": "npm-run-all -p -r startBackend serveWithDelay",
    "test:dev": "ng run project-perform:test:dev",
    "test:staging": "ng run project-perform:test:staging",
    "e2e:dev": "cross-env E2E=development ng run project-perform:e2e:dev",
    "e2eDevWithDelay": "npm-run-all delay e2e:dev",
    "e2eDevWithBackend": "npm-run-all -p -r startBackend e2eDevWithDelay",
    "e2e:staging": "cross-env E2E=staging ng run project-perform:e2e:staging",
    "e2e:production": "cross-env E2E=production ng run project-perform:e2e:production"
  },

