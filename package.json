  "comments": {
    "scripts": {
      "debug": "Run server allowing VSCode to connect to allow debug - only start from VSCode.",
      "isServerUp": "Pings the server.",
      "checkServer": "Checks if the local server is up & starts it if necessary.  This either finds the server started and exits with code 0 or starts a server in an external terminal and exits with code 0.  Use the Taskkill node.exe' task to kill the server if required.",
      "delDist": "Used before compilation to delete the compiled dist file.",
      "tscBackend": "Compiles backend.",
      "tscBackendWatch": "Compiles backend and recompiles on file change - used during development.",
      "uploadGCPFiles": "When a build is executed from GCP via a github trigger the secret files, and any empty directories, are not there => they are downloaded from GCP Storage.  This script uploads the files.",
      "gitPush": "uploads secrets to GCP Storage and does a git push to the remote origin - add the branch to be pushed to as a parameter, i.e.'npm run gitPush -- master'."
    }
  },
  "scripts": {
    "test": "mocha --config ./backend/.mocharc.json ./backend/dist/src/test/testSetup.js ./backend/dist/src/test/*.test.js ./backend/dist/src/**/test/*.test.js",
    "start": "node ./backend/dist/src/index.js",
    "debug": "node --inspect-brk=9229 ./backend/dist/src/index.js",
    "isServerUp": "ts-node --project ./backend/tsconfig.json ./backend/utils-build/isServerUp.ts",
    "checkServer": "ts-node --project ./backend/tsconfig.json ./backend/utils-build/checkServer.ts",
    "delDist": "ts-node ./backend/utils-build/delDistDir.ts ./backend/dist/",
    "tscBackend": "tsc --project ./backend/tsconfig.json",
    "tscBackendWatch": "tsc --watch --project ./backend/tsconfig.json",
    "compile": "npm-run-all delDist tscBackend",
    "prettierWrite": "prettier --write \"./**/*.ts\" \"./**/*.js\"",
    "eslint": "eslint --ext .js,.ts ./backend",
    "uploadGCPFiles": "ts-node ./backend/utils-build/syncGCPStorage.ts",
    "build": "npm-run-all prettierWrite eslint compile uploadGCPFiles",
    "gitPush": "npm run uploadGCPFiles && git push -u -v origin"
  },
